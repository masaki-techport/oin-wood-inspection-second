{
	"name": "Node Container",
	"dockerComposeFile": [
		"../../docker-compose.yml"
	],
	"service": "node-app",
	"shutdownAction": "none",
	"workspaceFolder": "/workspace",
	"postCreateCommand": "/usr/local/bin/npm install",
	"customizations": {
		"vscode": {
			"extensions": [
				"eamodio.gitlens",
				"GitHub.copilot"
			]
		}
	}
}